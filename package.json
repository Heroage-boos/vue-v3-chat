{
  "name": "vue-v3-chat2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build:dev": "vue-tsc --noEmit && vite build --mode staging",
    "build:prod": "vue-tsc --noEmit && vite build",
    "preview:dev": "npm build:dev && vite preview",
    "preview:prod": "npm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"src/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "prepare": "husky install",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "axios": "^1.4.0",
    "eslint-plugin-vue-scoped-css": "^2.4.0",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.36",
    "Prettier": "npm:prettier@^2.8.8",
    "sass": "^1.63.2",
    "swiper": "^9.3.2",
    "terser": "^5.18.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "unocss": "^0.53.1",
    "vite": "^4.3.5",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.4"
  }
}
